{
  "name": "node-backend",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:fastup-kit/node-backend.git",
  "author": "luchanso <luchansodev@gmail.com>",
  "license": "MIT",
  "scripts": {
    "generate": "yarn prisma:generate && yarn nexus:generate",
    "dev:app": "cross-env NODE_OPTIONS=--enable-source-maps yarn node build/index",
    "dev:db": "docker compose -f ./docker-compose-db.yml up",
    "dev:nexus": "nodemon -e ts --watch src --exec \"yarn nexus:generate\"",
    "start": "nodemon -e ts,d.ts --watch src --exec \"yarn nexus:generate && yarn build && yarn dev:app\"",
    "build": "mkdir -p build/generated && cp -r src/generated/prisma build/generated && tsc",
    "prisma:studio": "yarn prisma studio",
    "prisma:generate": "yarn pnpify prisma generate",
    "prisma:push": "yarn pnpify prisma db push",
    "prisma:migrate": "yarn pnpify prisma migrate dev",
    "prisma:reset": "yarn pnpify prisma migrate reset",
    "prisma:seed": "yarn prisma db seed",
    "nexus:generate": "yarn ts-node-dev --project tsconfig.json --transpile-only --no-notify ./src/schema/index.ts",
    "docker:build": "docker build . -t localhost:5000/project/node-backend",
    "docker:start": "PROJECT_NAME=project docker-compose up -d",
    "postinstall": "cat .env.example > .env",
    "prod:start": "node build/index",
    "prod:migrate": "yarn pnpify prisma migrate deploy"
  },
  "prisma": {
    "seed": "yarn ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/server": "4.0.0",
    "@as-integrations/fastify": "1.1.0",
    "@fastify/cookie": "8.3.0",
    "@luchanso/apollo-fastify": "0.10.0",
    "@prisma/client": "4.4.0",
    "@yarnpkg/pnpify": "4.0.0-rc.25",
    "apollo": "2.34.0",
    "apollo-server": "3.10.3",
    "apollo-server-core": "3.10.3",
    "apollo-server-errors": "3.3.1",
    "apollo-server-fastify": "3.10.3",
    "apollo-server-plugin-base": "3.6.3",
    "debug": "4.3.4",
    "dotenv": "16.0.3",
    "fastify": "4.9.1",
    "graphql": "16.6.0",
    "invariant": "2.2.4",
    "nexus": "1.3.0",
    "passport-google-oauth20": "2.0.0",
    "passport-http-bearer": "1.0.1",
    "stripe": "10.14.0",
    "tslib": "2.4.0",
    "typescript": "4.8.4"
  },
  "devDependencies": {
    "@types/debug": "4",
    "@types/invariant": "2.2.35",
    "@types/passport-google-oauth20": "2.0.11",
    "@types/passport-http-bearer": "1.0.37",
    "cross-env": "7.0.3",
    "nodemon": "2.0.20",
    "prisma": "4.4.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "2.0.0"
  },
  "packageManager": "yarn@3.2.4"
}
