{
  "name": "node-backend",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:fastup-kit/node-backend.git",
  "author": "luchanso <luchansodev@gmail.com>",
  "license": "MIT",
  "scripts": {
    "generate": "yarn prisma:generate && yarn nexus:generate",
    "dev:app": "cross-env NODE_OPTIONS=--enable-source-maps node build/index --inspect",
    "dev:db": "docker compose -f ./docker-compose.dev.yml up -d",
    "dev:nexus": "nodemon -e ts --watch src --exec \"yarn nexus:generate\"",
    "start": "nodemon -e ts,d.ts --watch src --exec \"yarn nexus:generate && yarn build && yarn dev:app\"",
    "build": "mkdir -p build/generated && cp -r src/generated build && tsc",
    "prisma:studio": "prisma studio",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate dev",
    "prisma:reset": "prisma migrate reset",
    "prisma:seed": "prisma db seed",
    "prod:daploy": "prisma migrate deploy",
    "nexus:generate": "GENERATE_NEXUS_TYPES=1 yarn ts-node --project tsconfig.json --transpileOnly ./src/schema/index.ts",
    "docker:build": "docker build . -t localhost:5000/project/node-backend",
    "docker:start": "PROJECT_NAME=project docker-compose up -d",
    "setup-env": "cat .env.example > .env",
    "prod:start": "yarn prod:migrate && node build/index"
  },
  "prisma": {
    "seed": "yarn ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/server": "4.0.0",
    "@as-integrations/fastify": "1.1.0",
    "@luchanso/apollo-fastify": "0.10.0",
    "@prisma/client": "4.16.2",
    "apollo": "2.34.0",
    "apollo-server": "3.10.3",
    "apollo-server-core": "3.10.3",
    "apollo-server-errors": "3.3.1",
    "apollo-server-fastify": "3.10.3",
    "busboy": "1.6.0",
    "cuid": "^2.1.8",
    "debug": "4.3.4",
    "dotenv": "16.0.3",
    "fastify": "4.9.1",
    "graphql": "16.6.0",
    "invariant": "2.2.4",
    "minio": "^7.0.32",
    "nexus": "1.3.0",
    "nexus-prisma": "^1.0.4",
    "passport-google-oauth20": "2.0.0",
    "passport-http-bearer": "1.0.1",
    "sharp": "^0.31.1",
    "stripe": "10.14.0",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "devDependencies": {
    "@types/busboy": "^1",
    "@types/cuid": "^2",
    "@types/debug": "4.1.7",
    "@types/invariant": "2.2.35",
    "@types/minio": "^7",
    "@types/passport-google-oauth20": "2.0.11",
    "@types/passport-http-bearer": "1.0.37",
    "@types/sharp": "^0",
    "cross-env": "7.0.3",
    "nodemon": "2.0.20",
    "prisma": "4.16.2",
    "ts-node": "10.9.1"
  },
  "packageManager": "yarn@3.4.1"
}
