{
  "name": "node-backend",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:fastup-kit/node-backend.git",
  "author": "luchanso <luchansodev@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev:app": "cross-env NODE_OPTIONS=--enable-source-maps yarn node build/index",
    "dev:db": "docker compose -f ./docker-compose-db.yml up",
    "start": "nodemon -e ts,d.ts --watch src --exec \"yarn build && yarn dev:app\"",
    "build": "tsc --incremental",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@luchanso/datasource-sql": "2.0.0",
    "@types/express": "^4.17.13",
    "@types/graphql-upload": "8.0.7",
    "apollo-server": "3.5.0",
    "apollo-server-core": "3.5.0",
    "apollo-server-errors": "3.3.1",
    "apollo-server-express": "3.5.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "graphql": "15.5.1",
    "graphql-upload": "13.0.0",
    "knex": "0.95.15",
    "pg": "8.7.1",
    "typescript": "4.5.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.3.0",
    "@graphql-codegen/introspection": "2.1.0",
    "@graphql-codegen/typescript": "2.4.1",
    "@graphql-codegen/typescript-resolvers": "2.4.2",
    "apollo": "2.33.9",
    "cross-env": "7.0.3",
    "nodemon": "^2.0.19"
  },
  "packageManager": "yarn@3.2.2"
}
